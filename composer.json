{
	"name": "quellenform/t3x-lib-ical",
	"type": "typo3-cms-extension",
	"description": "Download database-records as calendar items (VEVENT).",
	"keywords": [
		"TYPO3",
		"extension",
		"service",
		"ical"
	],
	"license": [
		"GPL-3.0-or-later"
	],
	"homepage": "https://extensions.typo3.org/extension/lib_ical",
	"support": {
		"source": "https://github.com/quellenform/t3x-lib-ical",
		"issues": "https://github.com/quellenform/t3x-lib-ical/issues"
	},
	"authors": [
		{
			"name": "Stephan Kellermayr",
			"email": "typo3@quellenform.at",
			"role": "Developer",
			"homepage": "https://www.quellenform.at/"
		}
	],
	"require": {
		"php": ">=8.0",
		"typo3/cms-core": "^11.5 || ^12"
	},
	"autoload": {
		"psr-4": {
			"Quellenform\\LibIcal\\": "Classes"
		}
	},
	"replace": {
		"typo3-ter/lib_ical": "self.version"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.typo3.org/"
		}
	],
	"extra": {
		"typo3/cms": {
			"extension-key": "lib_ical"
		},
		"branch-alias": {
			"dev-main": "0.x-dev"
		}
	}
}
